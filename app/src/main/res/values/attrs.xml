<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="colorDivider" format="color" />

    <attr name="colorBackgroundTransparent" format="color" />

    <declare-styleable name="FilterChip">
        <attr name="label" format="string" />
        <attr name="criterion" format="string" />
    </declare-styleable>

    <attr name="filterChipStyle" format="reference" />

    <declare-styleable name="CollapsingContainerView">
        <!-- whether the content is collapsed or expanded by default -->
        <attr name="expanded" format="boolean" />
        <!-- the duration of the collapsing / expanding animation -->
        <attr name="expand_duration" format="integer" />
    </declare-styleable>

    <declare-styleable name="RectProgressBar">
        <attr name="strokeWidth" />
        <attr name="strokeColor" />
        <attr name="cornerRadius" />
    </declare-styleable>

</resources>